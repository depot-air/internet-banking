    <style>
        /* This is the style for the trigger icon. The margin-bottom value causes the icon to shift down to center it. */
        .ui-datepicker-trigger { margin-right: -17px; margin-top: 1px; margin-bottom: -1px; }
        .selectTitle, .selectTitleChild { margin-top: -2px; margin-bottom: -22px; height: 30px; width: 80px; } /*margin-bottom: -20px; width: 60px; }*/
        .contactFromList { margin-bottom: -22px; height: 30px; width: 188px; }
