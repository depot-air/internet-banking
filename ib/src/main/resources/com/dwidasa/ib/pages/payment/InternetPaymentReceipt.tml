<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html t:type="layout" title="message:title" group="message:group"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

<div id="recipe_wrap">
    <form t:type="form" t:id="internetPaymentReceipt" clientValidation="submit">
        <div id="recipe">
            <div id="logo_blue"></div>
            <hr/>
            <div id="recipe_list">
                <table>
                    <tr class="odd">
                        <td class="first">
                            <label>${message:referenceNumber}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${internetPaymentView.referenceNumber}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:transactionDate}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>
                                <t:output value="internetPaymentView.transactionDate" format="longDate"/>
                                ${message:timeZone}
                            </label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:accountNumber}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${internetPaymentView.accountNumber}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:payment}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:internet}</label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:biller}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${internetPaymentView.billerName}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:customerReference}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${internetPaymentView.customerReference}</label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:referenceName}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${internetPaymentView.referenceName}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:amount}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                            	<t:output value="internetPaymentView.amount" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    
                    <tr class="odd">
                        <td>
                            <label>${message:bulanBayar}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${paidPeriods}</label>
                        </td>
                    </tr>
                   
                     <tr>
                        <td>
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;${message:amount1}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                            	<t:output value="internetPaymentView.amount1" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    <t:if test="internetPaymentView.amount2">
                     <tr  class="odd">
                        <td>
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;${message:amount2}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                            	<t:output value="internetPaymentView.amount2" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    
                    </t:if>
                    <t:if test="internetPaymentView.amount3">
                     <tr >
                        <td>
                            <label>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;${message:amount3}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                            	<t:output value="internetPaymentView.amount3" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    </t:if>
                    <tr>
                    	<td colSpan="2">
                   		</td>
                	</tr>
                     <tr class="odd">
                        <td>
                            <label>${message:admin}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                            	<t:output value="internetPaymentView.fee" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:total}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${message:curr}
                                <t:output value="internetPaymentView.total" format="formatter"/>
                            </label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:status}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${StatusDetil}</label>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <div id="note">
            ${message:noteReceipt}
        </div>
		<t:if test="fromHistory">
			<t:submit t:id="back" value="message:back" class="grey_btn left"/>
		</t:if>        
		
		<t:if test="StatusHistory">
		
		<p:else>
		
		<input type="button"
               onclick="popupInfo('/payment/internetPaymentPrint')"
               class="grey_btn right" t:type="any" value="message:print"/>
               
		</p:else>
		
		</t:if>   		
		
        
    </form>
</div>
</html>
