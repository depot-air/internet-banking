<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html t:type="layout" title="message:title" group="message:group"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">
<div id="recipe_wrap">
    <form t:type="form" t:id="refundDelimaReceipt" clientValidation="submit">
        <div id="recipe">
            <div id="logo_blue"></div>
            <hr/>
            <div id="recipe_list">
                <table>
                    <tr class="odd">
                        <td class="first">
                            <label>${message:referenceNumber}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label >${refundDelimaView.referenceNumber}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:date}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class = "last">
                            <label><t:output value="refundDelimaView.transactionDate" format="longDate"/>
                                ${message:timeZone}</label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:accountNumber}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${refundDelimaView.accountNumber}</label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:IdNumber}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${refundDelimaView.senderIdNumber}</label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:transferCode}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${refundDelimaView.customerReference}</label>
                        </td>
                    </tr><tr>
                        <td>
                            <label>${message:nominalTransfer}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label><t:output value="refundDelimaVIew.amount" format="formatter"/></label>
                        </td>
                    </tr>
                    <tr class="odd">
                        <td>
                            <label>${message:adminFee}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label><t:output value="refundDelimaView.providerFee" format="formatter"/></label>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label>${message:status}</label>
                        </td>
                        <td>
                            <label>|</label>
                        </td>
                        <td class="last">
                            <label>${status}</label>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <t:if test="fromHistory">
			<t:submit t:id="back" value="message:back" class="grey_btn left"/>
		</t:if>        
		<input type="button"
	   		   onclick="popupInfo('/delima/refundDelimaPrint')"
	   		   class="grey_btn right" t:type="any" value="message:print"/> 
    </form>
</div>
</html>