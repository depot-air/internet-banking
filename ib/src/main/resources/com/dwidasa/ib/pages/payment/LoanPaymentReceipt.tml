<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html t:type="layout" title="message:title"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
      xmlns:p="tapestry:parameter">

<div id="ct_right">
    <div id="page_title">
        <div class="ground_1"></div>
        <div class="ground_2"><p>${message:title}</p></div>
        <div class="ground_3"></div>
    </div>
    <form t:type="form">
        <t:paymentreceipt referenceNumber="loanPaymentView.referenceNumber"
                          transactionDate="loanPaymentView.transactionDate"
                          transactionType="loanPaymentView.transactionType" cardNumber="loanPaymentView.cardNumber"
                          accountNumber="loanPaymentView.accountNumber" billerName="loanPaymentView.billerName"
                          customerReference="loanPaymentView.customerReference" amount="loanPaymentView.amount"
                          status="status"/>
        <div id="form_submit_btn">
            <t:submit t:id="save" value="message:save" class="btn_style"/>
            <t:submit t:id="print" value="message:print" class="btn_style"/>
            <t:submit t:id="back" value="message:back" class="btn_style"/>
        </div>
    </form>
</div>
</html>
